@using DevOpsPortal.SessionObject
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Universal HealthCare Limited</title>
    @Styles.Render("~/Content/font-awesome.css")
    @Styles.Render("~/Content/font-awesome.min.css")
    @Styles.Render("~/Content/menu.css")
    @Styles.Render("~/Content/bootstrap.css")
    @Styles.Render("~/Content/bootstrap.min.css")
    @Styles.Render("~/Content/style.css")


    @Scripts.Render("~/bundles/modernizr")

    @{SingletonObject oSingleton = SingletonObject.GetCurrentSingleton();}
    @Html.Hidden("SessionID", @HttpContext.Current.Session.SessionID)
</head>
<body>
    <div class="fusion-header">
        <div class="fusion-row">
            <div class="fusion-logo" data-margin-top="31px" data-margin-bottom="31px" data-margin-left="0px" data-margin-right="0px">
                <img class="" src="~/Images/UniversalHealthCare.png" />
            </div>
            <div class="fusion-main-menu" style="">
                <nav>
                    <ul id="menu-aih_main_menu">
                        <li class="submenu">
                            @Html.ActionLink("Home", "Index", "Home")
                        </li>
                        <li class="submenu">
                            @Html.ActionLink("Services", "Index", "Home")
                            <ul>
                                <li>@Html.ActionLink("Corporate Wellness Programs", "Index", "Home")</li>
                                <li>@Html.ActionLink("Health Management Programs", "Index", "Home")</li>
                                <li>@Html.ActionLink("Health Dictionary", "Index", "Home")</li>
                                <li>@Html.ActionLink("Labkhoj: Compare Labs", "Index", "Home")</li>
                            </ul>
                        </li>
                        <li class="submenu">
                            @Html.ActionLink("Products", "Index", "Home")
                            <ul>
                                <li>@Html.ActionLink("Wellness Assessment", "Index", "Home")</li>
                                <li>@Html.ActionLink("Digital Health Wallet", "Index", "Home")</li>
                                <li>@Html.ActionLink("Quiz", "Index", "Home")</li>
                                <li>@Html.ActionLink("Trackers Unit", "Index", "Home")</li>
                                <li>@Html.ActionLink("Health Tracker App", "Index", "Home")</li>
                            </ul>
                        </li>
                        <li class="submenu">
                            @Html.ActionLink("About US", "Index", "Home")
                            <ul>
                                <li>@Html.ActionLink("Our Story", "Index", "Home")</li>
                                <li>@Html.ActionLink("Meet the Team", "Index", "Home")</li>
                                <li>@Html.ActionLink("Partner's Speak", "Index", "Home")</li>
                                <li>@Html.ActionLink("Achivements", "Index", "Home")</li>
                                <li>@Html.ActionLink("Career with Us", "Index", "Home")</li>
                            </ul>
                        </li>

                    </ul>
                </nav>
            </div>
        </div>
    </div>
    @*class="container body-content"*@
    <div>
        @RenderBody()
        <footer class="fusion-footer-widget-area fusion-widget-area">
            <div class="fusion-row">
                <div class="fusion-columns fusion-columns-4 fusion-widget-area">
                    <div class="fusion-column col-lg-3 col-md-3 col-sm-3">
                        <div id="text-3" class="fusion-footer-widget-column widget widget_text">
                            <h4 class="widget-title" data-fontsize="13" data-lineheight="13">Other Features</h4>			<div class="textwidget">
                                <p><a href="" target="_blank">Corporate Program</a></p>

                                <p><a href="" target="_blank">Heart Age Calculator</a></p>

                                <p><a href="" target="_blank">Health Quiz</a></p>

                                <p><a href="" target="_blank">BMI Calculator </a></p>
                            </div>
                            <div style="clear:both;"></div>
                        </div>
                    </div>
                    <div class="fusion-column col-lg-3 col-md-3 col-sm-3">
                        <div id="text-16" class="fusion-footer-widget-column widget widget_text">
                            <h4 class="widget-title" data-fontsize="13" data-lineheight="13">More About Us</h4>			<div class="textwidget">
                                <p><a href="" target="_blank"><i class="fa fa-rss"></i>&nbsp;&nbsp;&nbsp;&nbsp;Blog</a></p>

                                <p><a href="" target="_blank"><i class="fa fa-newspaper-o"></i>&nbsp;&nbsp;&nbsp;In the Press</a></p>

                                <p><a href="#"><i class="fa fa-comment-o"></i>&nbsp;&nbsp;&nbsp;FAQ</a></p>

                                <p><a href="" target="_blank">Career</a></p>

                                <p><a href="" target="_blank">Privacy Policy </a></p>
                            </div>
                            <div style="clear:both;"></div>
                        </div>
                    </div>

                    <div class="fusion-column fusion-column-last col-lg-3 col-md-3 col-sm-3">
                        <div id="contact_info-widget-4" class="fusion-footer-widget-column widget contact_info">
                            <h4 class="widget-title" data-fontsize="13" data-lineheight="13">Contact Us</h4>
                            <div class="contact-info-container">
                                <p class="address">202-A, 2nd Floor, Town Square, Viman Nagar, Pune - 411014, Maharashtra</p>

                                <p class="phone">Phone: 1800-123-7858</p>



                                <p class="email">Email: <a href="">info@UniversalHealthCare.com</a></p>

                            </div>
                            <div style="clear:both;"></div>
                        </div>
                    </div>
                    <div class="fusion-clearfix"></div>
                </div> <!-- fusion-columns -->
            </div> <!-- fusion-row -->

        </footer> <!-- fusion-footer-widget-area -->
        <footer id="footer" class="fusion-footer-copyright-area">
            <div class="fusion-row">
                <div class="fusion-copyright-content">
                    <div class="fusion-copyright-notice">
                        <div>Copyright 2015  | All Rights Reserved |  Universal HealthCare - Terms and Conditions</div>
                    </div>
                </div> <!-- fusion-fusion-copyright-content -->
            </div> <!-- fusion-row -->
        </footer> <!-- #footer -->

    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
